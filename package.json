  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=18.10.0"
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean:build prepare build:* compress",
    "debug": "cross-env NODE_ENV=production DEBUG_MODE=true npm-run-all clean:build prepare build:* compress preview",
    "build:client": "rollup --config rollup.config.js",
    "build:app-css": "node utils/app-css.js",
    "clean:dev": "node utils/clean.js dev",
    "clean:build": "node utils/clean.js build",
    "compress": "gzipper compress --deflate --brotli --gzip --gzip-level 9 --deflate-level 9 --brotli-quality 11 build",
    "dev": "npm-run-all --print-label --print-name clean:dev prepare -p dev:* serve",
    "dev:client": "rollup --config rollup.config.js --watch",
    "dev:app-css": "nodemon --watch src/app/scss --watch src/common/scss --ext scss --ignore src/common/scss/_config.scss utils/app-css.js -- dev",
    "icons:dev": "nodemon --watch src/common/icons.json utils/icon-importer.js",
    "icons:build": "node utils/icon-importer.js",
    "prepare": "npm run scss:config",
    "preview": "cross-env NODE_ENV=production npm-run-all --print-label --print-name serve",
    "scss:config": "node utils/config-scss.js",
    "serve": "nodemon server.js"
  },
  "dependencies": {
    "@hapi/accept": "^6.0.1",
    "@hapi/boom": "^10.0.0",
    "@sindresorhus/is": "^5.4.1",
    "chalk": "^5.2.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.31.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^7.0.7",
    "express-minify-html": "^0.12.0",
    "front-matter": "^4.0.2",
    "handlebars-helpers": "^0.10.0",
    "http2-express-autopush": "^1.0.5",
    "http2-express-bridge": "^1.0.7",
    "js-cookie": "^3.0.1",
    "lit": "^2.5.0",
    "lodash-es": "^4.17.21",
    "mime-types": "^2.1.35",
