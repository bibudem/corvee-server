{
  "name": "corvee-server",
  "version": "0.0.0",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=18.10.0"
  },
  "targets": {
    "cv-server-js:dev": {
      "distDir": "./dev/js",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-server/home.js",
        "src/cv-server/section.js"
      ]
    },
    "cv-server-css:dev": {
      "distDir": "./dev/css",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-server/scss/cv-server.scss"
      ]
    },
    "components:dev": {
      "distDir": "./dev",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-report/cv-report.js",
        "src/cv-switch/cv-switch.js"
      ]
    },
    "cv-server-js:build": {
      "distDir": "./build/js",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-server/home.js",
        "src/cv-server/section.js"
      ],
      "sourceMap": false
    },
    "cv-server-css:build": {
      "distDir": "./build/css",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-server/scss/cv-server.scss"
      ],
      "sourceMap": false
    },
    "components:build": {
      "distDir": "./build",
      "isLibrary": false,
      "context": "browser",
      "source": [
        "src/cv-report/cv-report.js",
        "src/cv-switch/cv-switch.js"
      ],
      "sourceMap": false
    }
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "clean:dev": "npm-run-all clean:dev:rimraf clean:dev:make-dir",
    "clean:build": "npm-run-all clean:build:rimraf clean:build:make-dir",
    "clean:dev:rimraf": "rimraf dev",
    "clean:dev:make-dir": "make-dir dev",
    "clean:build:rimraf": "rimraf build",
    "clean:build:make-dir": "make-dir build",
    "dev": "npm-run-all --print-label --print-name clean:dev -p sprite:watch dev:*",
    "dev:client": "parcel watch --no-cache --target cv-server-js:dev --target cv-server-css:dev --target components:dev",
    "dev:server": "nodemon",
    "build": "cross-env NODE_ENV=production npm-run-all clean:build sprite:build build:*",
    "build:client": "parcel build --target cv-server-js:build --target cv-server-css:build --target components:build",
    "preview": "cross-env NODE_ENV=production npm-run-all --print-label --print-name build -p preview:*",
    "preview:api": "nodemon",
    "serve": "node index.js",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "sprite:build": "node scripts/sprite-generator.js",
    "sprite:watch": "nodemon --config nodemon.sprite.json scripts/sprite-generator.js"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.26.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^6.0.6",
    "express-minify": "^1.0.0",
    "front-matter": "^4.0.2",
    "handlebars-helpers": "^0.10.0",
    "js-cookie": "^3.0.1",
    "lit": "^2.4.1",
    "make-dir-cli": "^3.0.0",
    "mongoose": "^6.7.2",
    "rimraf": "^3.0.2",
    "sirv": "^2.0.2",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {
    "@evanminto/sticky-sentinel-element": "^0.2.0",
    "@parcel/transformer-inline-string": "^2.8.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@popperjs/core": "^2.11.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "algoliasearch": "^4.14.2",
    "bootstrap": "^5.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.7.0",
    "instantsearch.js": "^4.49.1",
    "moment": "^2.29.4",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.0",
    "parcel-transformer-lit-sass": "^0.1.0",
    "popmotion": "^11.0.5",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "svg-sprite": "^2.0.2",
    "svgo": "^2.8.0",
    "underscore": "^1.13.6",
    "vinyl": "^3.0.0"
  }
}